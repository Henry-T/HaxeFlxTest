  <!-- Other classes to be compiled into your SWF -->
  <classpaths>
    <class path="source" />
    <class path="C:\HaxeToolkit\haxe/lib\openfl/1,0,0/" />
    <class path="C:\HaxeToolkit\haxe/lib\flixel/2,0,0-alpha,2/" />
    <class path="bin/flash/haxe" />
  </classpaths>
  <!-- Build options -->
  <build>
    <option directives="nme4=1&#xA;tools=1&#xA;nme_ver=4.0.2&#xA;nme&#xA;nme_install_tool=1&#xA;display&#xA;web" />
    <option flashStrict="False" />
