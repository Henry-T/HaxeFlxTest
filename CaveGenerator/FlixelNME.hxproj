  </library>
  <!-- Paths to exclude from the Project Explorer tree -->
  <hiddenPaths>
    <!-- example: <hidden path="..." /> -->
  </hiddenPaths>
  <!-- Executed before build -->
  <preBuildCommand>haxelib run openfl test "FlixelNME.xml" neko
haxelib run openfl test "FlixelNME.xml" windows</preBuildCommand>
